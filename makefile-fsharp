FILE=Something
MONO=/suphys/jpal8929/bin/mono
FSC=/suphys/jpal8929/FSharp-2.0.0.0/bin/fsc.exe
FSI=/suphys/jpal8929/FSharp-2.0.0.0/bin/fsi.exe
COMPILER=${MONO} ${FSC}
MONOOPTS= --optimize=all
FSHARPOPTS=--optimize+ --nologo --checked-

${FILE}.exe: ${FILE}.fs
	${COMPILER} -o ${FILE}.exe ${FILE} ${FSHARPOPTS}

run: ${FILE}.exe
	mono ${MONOOPTS} ${FILE}.exe 

